// Arreglo al cubo

Funcion Imprime(X)
	Definir i Como Entero
	Para i=1 Hasta 20 Hacer
		Escribir X[i]
	FinPara
	Escribir ""
FinFuncion


Algoritmo ImprimeArreglo
	Definir A,B,i Como Entero
	Dimension A[20], B[20]
	
	Para i=1 Hasta 20 Hacer
		A[i] = Aleatorio(1,100)
		B[i] = A[i] * A[i] * A[i] 
	FinPara
	
	
	Escribir "Elementos del Arreglo A"
	Imprime(A)
	
	Escribir "Elementos del Arreglo B"
	Imprime(B)
	
	
FinAlgoritmo
