// Imprimir un arreglo usando una función

Funcion Imprime(X)
	Definir i Como Entero
	Para i=1 Hasta 5 Hacer
		Escribir X[i]
	FinPara
	Escribir ""
FinFuncion

Algoritmo ImprimeArreglo
	Definir A,B Como Entero
	Dimension A[5], B[5]
	
	A[1]=10
	A[2]=20
	A[3]=30
	A[4]=40
	A[5]=50
	
	B[1]=50
	B[2]=40
	B[3]=30
	B[4]=20
	B[5]=10
	
	Escribir "Elementos del Arreglo A"
	Imprime(A)
	
	Escribir "Elementos del Arreglo B"
	Imprime(B)
	
FinAlgoritmo
