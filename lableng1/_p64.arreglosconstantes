// Imprime arreglos constantes

#include<stdio.h>

void imprime(int X[]) {
	int i;
	for(i=0; i<=4; i++) {
		printf("%d ",X[i]);
	}
	printf("\n\n");
}


main() {
	int A[]={10,20,30,40,50};
	int B[]={25,35,45,55,65};
	
	printf("Contenido del arreglo A\n");
	imprime(A);
	printf("Contenido del arreglo B\n");
	imprime(B);
	printf("\nProceso terminado ....\n");
	
}
